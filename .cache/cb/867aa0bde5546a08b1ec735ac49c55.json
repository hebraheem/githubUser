ecoration: none;\n  list-style: none;\n  display: inline-block;\n  padding: 10px 5px;\n}\na {\n  text-decoration: none;\n  color: white;\n  font-size: 14px;\n}\n.right-icons {\n  display: flex;\n  align-items: center;\n  justify-content: space-around;\n}\n.drop-down {\n  margin-right: 20px;\n  margin-left: 5px;\n}\na:hover,\n.select:hover,\n.git-icon:hover,\n.add-icon:hover,\n.bell:hover {\n  color: rgba(240, 246, 252, 0.7);\n}\n.bell {\n  margin-right: 20px;\n}\n.user-image {\n  width: 30px;\n  height: 30px;\n  border-radius: 100%;\n}\n\n/* second header section */\n.second-header {\n  width: 100%;\n  height: 100px;\n  box-shadow: 2px 1px 3px rgba(128, 128, 128, 0.2);\n}\n.nav-bar {\n  width: 75%;\n  float: right;\n  height: 70px;\n}\n.active {\n  color: black;\n  border-bottom: 3px solid orange;\n}\n.activelink {\n  color: black !important;\n}\n.rep-count {\n  color: black;\n  background-color: rgba(128, 128, 128, 0.5);\n  font-size: 14px;\n  padding: 5px 10px;\n  border-radius: 10px;\n}\n.nav-ul .nav-li {\n  display: inline-block;\n  padding: 38px;\n  padding-top: 55px;\n  padding-bottom: 21px;\n}\n.nav-li a {\n  color: grey;\n  font-size: 18px;\n}\n.nav-icon {\n  margin-right: 5px;\n}\n.nav-li a:hover {\n  color: black;\n}\n/* body content section */\n.body-content {\n  display: flex;\n  justify-content: space-between;\n}\n.left {\n  margin-left: 50px;\n}\n.avatar {\n  width: 300px;\n  height: 300px;\n  border-radius: 100%;\n  top: -25px;\n  position: relative;\n  background-color: grey;\n}\n.name {\n  font-size: 2rem;\n}\n.user-name {\n  font-size: 16px;\n  color: grey;\n  padding-bottom: 20px;\n}\n.desc,\n.profession {\n  font-size: 14px;\n  width: 200px;\n}\n.right {\n  width: 100%;\n  padding: 30px;\n}\n.rep-input {\n  width: 100%;\n  margin-bottom: 10px;\n  height: 40px;\n  border-radius: 8px;\n  outline: none;\n  padding-left: 10px;\n  border: solid rgba(128, 128, 128, 0.2) 1px;\n}\nhr {\n  margin: 20px 0;\n  border: rgba(128, 128, 128, 0.2) 1px solid;\n  height: 1px;\n}\n.count-hr {\n  width: 260px;\n}\n.count {\n  font-weight: bold;\n  padding-right: 5px;\n}\n.pinable {\n  width: 30px;\n  height: 30px;\n  border-radius: 100%;\n  position: absolute;\n  top: 350px;\n  left: 315px;\n}\n\n/* repo info */\n.repo-info {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n.repo-name {\n  color: #58a6ff;\n  padding-bottom: 10px;\n  cursor: pointer;\n}\n.repo-desc {\n  font-size: 16px;\n  color: rgba(0, 0, 0, 0.7);\n  padding-bottom: 10px;\n}\n.repo-language {\n  color: rgba(0, 0, 0, 0.7);\n}\np:empty {\n  width: 10px;\n  height: 10px;\n  border-radius: 100%;\n  margin-right: 5px;\n}\n.repo-lang-icon {\n  margin-left: 20px;\n}\n.updated {\n  padding-left: 10px;\n}\n.star-btn {\n  width: 70px;\n  height: 25px;\n  outline: none;\n  border: 1px;\n  border-radius: 3px;\n  background-color: rgba(35, 65, 100, 0.1);\n  cursor: pointer;\n}\n.no-user {\n  text-align: center;\n  font-size: 200%;\n  color: #58a6ff;\n  padding-top: 10%;\n}\n\n/* home section */\n\n.home-section {\n  height: 60vh;\n  background-color: #fff;\n  width: 40%;\n  margin-top: 10%;\n  text-align: center;\n  padding: 20px;\n  border-radius: 5px;\n  box-shadow: 1px 2px 1px rgba(35, 65, 100, 0.1);\n}\n.body {\n  background-color: rgba(128, 128, 128, 0.2);\n  display: grid;\n  place-items: center;\n}\n#search {\n  height: 40px;\n  width: 80%;\n  padding-left: 10px;\n  outline: none;\n  border-radius: 5px;\n}\n.sub-btn {\n  width: 15%;\n  height: 40px;\n  background-color: rgba(35, 65, 100, 0.3);\n  outline: none;\n  border: none;\n  border-radius: 5px;\n}\n.search-user {\n  font-size: 250%;\n  padding: 50px 0 100px 0;\n}\n\n@media only screen and (max-width: 820px) {\n  .home-section {\n    height: 70vh;\n    width: 90%;\n    margin-top: 20%;\n  }\n  .body-content {\n    display: block;\n  }\n  .nav-ul .nav-li {\n    display: none;\n  }\n  .top-ul .top-li {\n    display: none;\n  }\n  .input {\n    width: 100px;\n    transition: width 1s;\n  }\n  .input:focus {\n    width: 150px;\n  }\n  .left {\n    text-align: center;\n    margin-left: 0;\n  }\n  .profession {\n    width: 100%;\n  }\n}\n\n@media only screen and (max-width: 935px) {\n  .nav-ul .nav-li {\n    padding: 38px 10px;\n  }\n  .nav-ul .nav-li a {\n    padding: 10px;\n  }\n}\n"},"lineCount":311}},"error":null,"hash":"694536fb845a8b0dc1af3bf35230713f","cacheData":{"env":{}}}